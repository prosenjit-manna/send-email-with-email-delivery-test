{
  "name": "email-deliverability-tester",
  "version": "1.0.0",
  "description": "A comprehensive email deliverability testing utility that validates email format, domain existence, DNS records, and tests actual delivery via AWS SES and SMTP",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "email-test": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepublishOnly": "npm run build",
    "example:basic": "ts-node examples/basic-usage.ts",
    "example:advanced": "ts-node examples/advanced-usage.ts",
    "cli": "ts-node src/cli.ts"
  },
  "keywords": [
    "email",
    "deliverability",
    "validation",
    "aws-ses",
    "smtp",
    "email-testing",
    "dns",
    "mx-records",
    "spf",
    "dmarc",
    "email-verification"
  ],
  "author": "prosenjit-itobuz",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prosenjit-manna/send-email-with-email-delivery-test.git"
  },
  "bugs": {
    "url": "https://github.com/prosenjit-manna/send-email-with-email-delivery-test/issues"
  },
  "homepage": "https://github.com/prosenjit-manna/send-email-with-email-delivery-test",
  "dependencies": {
    "@aws-sdk/client-ses": "^3.864.0",
    "nodemailer": "^6.10.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.11",
    "@types/nodemailer": "^6.4.18",
    "dotenv": "^16.6.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "peerDependencies": {
    "dotenv": "^16.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
